[color]

trans = #00000000
white = #FFFFFF
black = #000000

;bg = #BF2f343f
bg = #592f343f
fg = #FFFFFF
fg-alt = #A9ABB0

acolor = #00acc1
curgent = #e53935
coccupied = #039be5

## Material Colors

red = #e53935
pink = #d81b60
purple = #8e24aa
deep-purple = #5e35b1
indigo = #3949ab
blue = #1e88e5
light-blue = #039be5
cyan = #00acc1
teal = #00897b
green = #43a047
light-green = #7cb342
lime = #c0ca33
yellow = #fdd835
amber = #ffb300
orange = #fb8c00
deep-orange = #f4511e
brown = #6d4c41
grey = #757575
blue-gray = #546e7a
pastel-blue = #bdecb6
pastel-red = #dedede
pink-red = #e60053
pastel-green = #b8f3af

;==========================================================

[bar/mybar]
;monitor = LVDS1
width = 100%
height = 14
offset-x = 2%
offset-y = 2%
bottom = false
fixed-center = false
line-size = 2
;radius = 0.0

background = ${color.bg}
foreground = ${color.fg}

border-size = 3
border-color = ${color.bg}

module-margin-left = 1
module-margin-right = 1

;==========================================================

font-0 = "DejaVuSans:size=8;2"
font-1 = "Material Icons:size=8;3"
font-2 = "Weather Icons:size=8;3"
font-3 = "Monaco:size=7;2"
font-4 = "Monaco:size=8;2"

;==========================================================

cursor-click = pointer
cursor-scroll = ns-resize

;tray-position = right
;tray-padding = 0
;tray-maxsize = 16

scroll-up = i3wm-wsnext
scroll-down = i3wm-wsprev

;== Module Location ========================================================

modules-left =i3 xkeyboard
modules-center = xwindow 
modules-right = openweathermap-forecast cpu temperature memory xbacklight volume battery wlan eth date powermenu
 
