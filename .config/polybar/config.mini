[module/xkeyboard]
type = internal/xkeyboard
blacklist-0 = num lock

label-indicator-padding = 2
label-indicator-margin = 1
label-indicator-background = ${color.acolor}
label-layout-foreground = ${color.bg}

;==========================================================

[module/xwindow]
type = internal/xwindow
label = %title:...%
label-maxlen = 50
label-empty = i3wm
label-font = 5

;==========================================================

[module/openweathermap-forecast]
type = custom/script
exec = ~/.config/scripts/openweathermap-forecast.sh
label-foreground = #A4C2F4
interval = 600
label-font = 4

[module/cpu]
type = internal/cpu
interval = 3
format-prefix = ""
format-foreground = ${color.teal}
format-underline = 
format-prefix-font = 5

label = %percentage:2%%


[module/temperature]
type = internal/temperature
thermal-zone = 0
warn-temperature = 60

format = <ramp> <label>
format-underline = 
format-warn = <ramp> <label-warn>
format-warn-underline = 
format-warn-foreground = ${color.pastel-red}

label = %temperature-c%
label-warn = %temperature-c%
label-warn-foreground = ${color.curgent}

;format-font = 5

ramp-0 = ""
ramp-1 = ""
ramp-2 = ""
ramp-foreground = 

[module/memory]
type = internal/memory
format-prefix = ""
; Seconds to sleep between updates
; Default: 1
interval = 3
; Available tags:
;   <label> (default)
;   <bar-used>
;   <bar-free>
;   <ramp-used>
;   <ramp-free>
;   <bar-swap-used>
;   <bar-swap-free>
;   <ramp-swap-used>
;   <ramp-swap-free>
format = <label> 


format-prefix-font = 5
format-foreground = ${color.pastel-green}

; Available tokens:
;   %percentage_used% (default)
;   %percentage_free%
;   %gb_used%
;   %gb_free%
;   %gb_total%
;   %mb_used%
;   %mb_free%
;   %mb_total%
;   %percentage_swap_used%
;   %percentage_swap_free%
;   %mb_swap_total%
;   %mb_swap_free%
;   %mb_swap_used%
;   %gb_swap_total%
;   %gb_swap_free%
;   %gb_swap_used%

label = %gb_used% / %gb_swap_used%

; Only applies if <bar-used> is used
bar-used-indicator =
bar-used-width = 10
bar-used-foreground-0 = #55aa55
bar-used-foreground-1 = #557755
bar-used-foreground-2 = #f5a70a
bar-used-foreground-3 = #ff5555
bar-used-fill = ■
bar-used-empty = ■
bar-used-empty-foreground = #444444

; Only applies if <ramp-used> is used
ramp-used-0 = ▁
ramp-used-1 = ▂
ramp-used-2 = ▃
ramp-used-3 = ▄
ramp-used-4 = ▅
ramp-used-5 = ▆
ramp-used-6 = ▇
ramp-used-7 = █

; Only applies if <ramp-free> is used
ramp-free-0 = ▁
ramp-free-1 = ▂
ramp-free-2 = ▃
ramp-free-3 = ▄
ramp-free-4 = ▅
ramp-free-5 = ▆
ramp-free-6 = ▇
ramp-free-7 = █

;==========================================================

[module/mpd]
type = internal/mpd
;format-online = <toggle> <label-song> 
format-online =  <label-song> 

icon-play = 
icon-pause = 

label-song-maxlen = 25
label-song-ellipsis = true

;==========================================================

[module/xbacklight]
type = internal/xbacklight

enable-scroll = true

; Tags disponíveis: 
;    <label> (padrão) 
;    <ramp> 
;    <bar> 
;format = <ramp> <bar>
format = <ramp> <label> 

; Available tokens:
;   %percentage% (default)
label = %percentage%%

; Only applies if <ramp> is used
ramp-0 ="🌕"
ramp-1 ="🌔"
ramp-2 ="🌓"
ramp-3 ="🌒"
ramp-4 ="🌑"

bar-width = 10
bar-indicator = 
bar-indicator-foreground = #fff
bar-indicator-font = 2
bar-fill ="%{A1:~/.config/scripts/controlbright.sh --control:}■%{A}"
bar-fill-font = 2
bar-fill-foreground = #9f78e1
bar-empty ="%{A1:~/.config/scripts/controlbright.sh --control:}■%{A}"
bar-empty-font = 2
bar-empty-foreground = ${colors.foreground-alt}

[module/battery]
type = internal/battery
full-at = 98
time-format = %H:%M
battery = BAT0
adapter = ADP1
format-charging = <animation-charging> <label-charging>
label-discharging-foreground = ${color.red}
label-charging-foreground = ${color.green}
;label-charging = %percentage%% (%time%)
;label-charging = %percentage%%
format-discharging = <animation-discharging> <label-discharging>
;label-discharging = %percentage%% (%time%)
;label-discharging = %percentage%%
;format-full = <label-full>
;       

format-full-prefix = ""
ramp-capacity-0 =
ramp-capacity-1 =
ramp-capacity-2 =
ramp-capacity-3 =
;ramp-capacity-4 =

ramp-capacity-0-foreground = ${color.red}
ramp-capacity-foreground   = ${color.fg}
bar-capacity-width = 10

animation-charging-0 = ""
animation-charging-1 = ""
animation-charging-2 = ""
;animation-charging-3 = ""
;animation-charging-4 = ""
animation-charging-foreground = ${color.green}

animation-charging-framerate = 750

animation-discharging-0 =""
animation-discharging-1 =""
animation-discharging-2 =""
;animation-discharging-3 =""
animation-discharging-foreground = ${color.red}

animation-discharging-framerate = 750

[module/volume]
type = internal/alsa
;speaker-mixer = Speaker
;headphone-mixer = Headphone
;headphone-id = 16
;mapped = true
enable-scroll = true
format-volume = <ramp-volume> <label-volume>
label-volume = %percentage%%
format-muted-prefix = " "
label-muted = MUTE
label-muted-foreground = #666
format-muted-foreground = #e60053

ramp-volume-0 = 
ramp-volume-1 = 
ramp-volume-2 = 
ramp-volume-3 = 
ramp-volume-4 = 
ramp-volume-5 = 
ramp-volume-6 = 

[module/wlan]
type = internal/network
interface = wlp2s0b1
interval = 30

format-connected = <ramp-signal> <label-connected>
format-connected-underline = 

label-connected ="%{A1:~/.config/scripts/scriptwicd.sh:}%signal%%%{A}"
;%essid% %downspeed:9%
;format-disconnected =
format-disconnected = <label-disconnected>
format-disconnected-underline = 
;label-disconnected = %ifname% disconnected
label-disconnected ="%{A1:terminator -e wicd-curses &:} %{A}"
label-disconnected-foreground = #666

ramp-signal-0 = "%{A1:terminator -e wicd-curses &:} %{A}"
ramp-signal-1 = "%{A1:terminator -e wicd-curses &:} %{A}"
ramp-signal-2 = "%{A1:terminator -e wicd-curses &:} %{A}"
ramp-signal-3 = "%{A1:terminator -e wicd-curses &:} %{A}"
ramp-signal-4 = "%{A1:terminator -e wicd-curses &:} %{A}"
ramp-signal-foreground = 

[module/eth]
type = internal/network
interface = enp3s0
interval = 3.0

format-connected-underline = 
format-connected-prefix = " "
format-connected-prefix-foreground = 
label-connected = "%{A1:~/.config/scripts/scriptwicd.sh:} %local_ip%%{A}"

format-disconnected =


[module/date]
type = internal/date
interval = 30
label = "%{A3:~/.config/scripts/popup-calendar.sh --popup:} %date% %time%%{A}"
time =  %H:%M %p
time-alt = %H:%M:%S
;time-alt =  %Y-%m-%d
date-alt =  "%d-%m-%Y"

[module/powermenu]
type = custom/text
content = 
content-padding = 2
content-background = ${color.acolor}
content-foreground = ${color.fg}
click-left = ~/.config/scripts/shutdown_menu
click-right = ~/.config/scripts/shutdown_menu

;== EOF ========================================================
